    "page_number": 2,
    "bbox": [
      61.56,
      526.69,
      548.26,
      565.03
    ],
    "text": "Showers or thundershowers will occur at several places in the sea areas off the coast extending from Trincomalee to Galle via Kankasanthurai, Mannar and Colombo.",
    "fonts": [
      "Calibri"
    ],
    "sizes": [
      6.0,
      12.0
    ]
  },
