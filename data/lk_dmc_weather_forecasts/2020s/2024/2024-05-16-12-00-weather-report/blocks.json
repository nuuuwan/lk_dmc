  {
    "page_number": 0,
    "bbox": [
      67.58,
      541.51,
      534.17,
      553.51
    ],
    "text": "Showers or thundershowers will occur at several places elsewhere of the island after 1.00 p.m.",
    "fonts": [
      "Calibri"
    ],
    "sizes": [
      12.0
    ]
  },
  {
    "page_number": 0,
    "bbox": [
      67.58,
      562.15,
      536.92,
      588.91
    ],
    "text": "Heavy showers about 100mm are likely at some places in Western, Central, Sabaragamuwa and Uva provinces.",
    "fonts": [
      "Calibri",
      "Calibri-Bold",
      "IskoolaPota"
    ],
    "sizes": [
      12.0
    ]
  },
